{
  "name": "vibrolab",
  "version": "0.1.0",
  "description": "A video vibration toolbox",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RLado/Vibrolab.git"
  },
  "keywords": [
    "MotionMagnification",
    "DeepLearning",
    "SHM",
    "ModalAnalysis"
  ],
  "author": "Ricard Lado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RLado/Vibrolab/issues"
  },
  "homepage": "https://github.com/RLado/Vibrolab#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "electron": "^19.0.8"
  },
  "dependencies": {
    "chart.js": "^3.8.0",
    "csvtojson": "^2.0.10",
    "dialogs": "git://github.com/RLado/dialogs_js.git",
    "electron-squirrel-startup": "^1.0.0",
    "get-video-dimensions": "^1.0.0",
    "image-size": "^1.0.2",
    "python-shell": "^3.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "vibrolab"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
